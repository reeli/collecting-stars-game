{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "pretty": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": ".tmp",
    "baseUrl": ".",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
